{
  "name": "sense-on",
  "version": "0.1.0",
  "type": "module",
  "description": "On-device face tracking and attention monitoring for the web. MediaPipe-powered, 100% client-side, privacy-first.",
  "main": "./dist-lib/index.js",
  "module": "./dist-lib/index.js",
  "types": "./dist-lib/types/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist-lib/index.js",
      "types": "./dist-lib/types/lib.d.ts"
    },
    "./core": {
      "import": "./dist-lib/core.js",
      "types": "./dist-lib/types/core/index.d.ts"
    }
  },
  "files": [
    "dist-lib",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "keywords": [
    "face-tracking",
    "attention",
    "mediapipe",
    "head-pose",
    "eye-tracking",
    "drowsiness",
    "web-worker",
    "privacy",
    "on-device",
    "ear"
  ],
  "author": "kimseungdae <kocacolla@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimseungdae/sense-on.git"
  },
  "homepage": "https://github.com/kimseungdae/sense-on#readme",
  "bugs": {
    "url": "https://github.com/kimseungdae/sense-on/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vite build --config vite.config.lib.ts && vue-tsc --project tsconfig.lib.json",
    "test": "vitest run",
    "setup": "tsx scripts/setup-mediapipe.ts",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "@mediapipe/tasks-vision": "^0.10.0",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mediapipe/tasks-vision": "^0.10.32",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.8.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue": "^3.3.0",
    "vue-router": "^4.6.4",
    "vue-tsc": "^3.1.5"
  }
}
